# Disable GNOME user extensions installation & usage
# Only GNOME system extensions are trusted if installed

[org.gnome.shell]
allow-extension-installation=false
favorite-apps = ['chromium-browser.desktop', 'org.gnome.Nautilus.desktop', 'org.gnome.Ptyxis.desktop', 'com.github.tchx84.Flatseal.desktop', 'io.github.flattool.Warehouse.desktop']
enabled-extensions = []

[org.gnome.desktop.interface]
gtk-theme='adw-gtk3'
font-antialiasing="rgba"
monospace-font-name="Monospace 12"